{
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ],
    "env": {
      "browser": true,
      "node": true,
      "jquery": true,
      "es6": true
    },
    "ecmaFeatures": {
      "arrowFunctions": true,
      "blockBindings": true,
      "classes": true,
      "defaultParams": true,
      "destructuring": true,
      "modules": true,
      "restParams": true,
      "spread": true,
      "superInFunctions ": true,
      "templateStrings  ": true,
      "jsx": true
    },
    "rules": {
      "strict": 0,
      "comma-dangle": [2, "always-multiline"], "no-cond-assign": [2, "always"],
      "no-dupe-args": 2,
      "no-dupe-keys": 2,
      "no-duplicate-case": 2,
      "no-empty": 2,
      "no-func-assign": 2,
      "no-inner-declarations": 2,
      "no-multi-spaces": 0,
      "no-unreachable": 2,
      "no-unexpected-multiline": 2,
      "block-scoped-var": 0,
      "consistent-return": 2,
      "curly": [2, "multi-line"],
      "default-case": 2,
      "eqeqeq": 2,
      "no-eval": 2,
      "no-labels": 2,
      "no-lone-blocks": 2,
      "no-fallthrough": 2,
      "no-return-assign": 2,
      "no-shadow": 2,
      "brace-style": [2, "stroustrup", { "allowSingleLine": true }],
      "no-unused-vars": 2,
      "no-use-before-define": 0,
      "array-bracket-spacing": [1, "never"], "camelcase": [1, {"properties": "always"}],
      "comma-spacing": [1, {"before": false, "after": true } ], "comma-style": [1, "last"],
      "indent": [1, 4, {"SwitchCase": 1} ],
      "no-lonely-if": 1,
      "new-cap": 0,
      "no-spaced-func": 1,
      "no-trailing-spaces": 1,
      "no-underscore-dangle": 0,
      "object-curly-spacing": 0,
      "operator-linebreak": [1, "before"],
      "quote-props": [1, "as-needed"],
      "quotes": [1, "single", "avoid-escape"],
      "semi": [1, "always"],
      "space-after-keywords": [1, "always"],
      "space-before-blocks": [1, "always"],
      "space-before-function-paren": [1, "never"],
      "space-in-parens": [1, "never"],
      "space-infix-ops": 1,
      "space-return-throw-case": 1,
      "key-spacing": 0,

      "arrow-parens": [1, "as-needed"],
      "arrow-spacing": 2,
      "constructor-super": 2,
      "no-const-assign": 2,
      "no-dupe-class-members": 2,
      "no-this-before-super": 2,
      "no-var": 2,
      "object-shorthand": 2,
      "prefer-arrow-callback": 2,
      "prefer-const": 2,
      "prefer-template": 0,

      "react/display-name": [0, {"acceptTranspilerName": true } ],
      "react/jsx-boolean-value": [1, "always"],
      "react/jsx-curly-spacing": 0,
      "react/jsx-no-undef": 1,
      "jsx-quotes": [1, "prefer-single"],
      "react/jsx-sort-prop-types": 0,
      "react/jsx-sort-props": 0,
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1,
      "react/no-did-mount-set-state": 1,
      "react/no-did-update-set-state": 1,
      "react/no-multi-comp": 0,
      "react/no-unknown-property": 1,
      "react/prop-types": 1,
      "react/react-in-jsx-scope": 1,
      "react/require-extension": 1,
      "react/self-closing-comp": 1,
      "react/sort-comp": 0,
      "react/wrap-multilines": 1
    }
  }